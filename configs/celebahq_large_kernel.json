{
  "_comment": "Asymmetric autoencoder with no linear latent layer - keeps spatial structure throughout, using 511 filters at latent; kernel_size set to 13 for test",
  "description": "Asymmetric CelebA-HQ Taxonomic Autoencoder: reduced encoder layers (256x256->16x16) and 4 decoder layers (16x16->256x256) with no linear bottleneck (kernel_size=13)",
  "experiment_name": "celebahq_large_kernel",
  "data": {
    "data_root": "./data/celeba_hq",
    "batch_size": 64,
    "num_workers": 4,
    "val_split": 0.1,
    "train_subset": null,
    "val_subset": null,
    "_comment_subset": "Set train_subset/val_subset to an integer (e.g., 5000) to train on a smaller subset for faster iteration. null = use full dataset"
  },
  "model": {
    "latent_dim": 255,
    "temperature": 1.0,
    "use_maxpool": false,
    "output_activation": "sigmoid",
    "encoder_layers": [
      {
        "layer_type": "taxonomic_conv",
        "n_layers": 5,
        "kernel_size": 13,
        "stride": 2,
        "padding": 6,
        "_comment": "256x256x3 -> 128x128x(2^5-1) = 128x128x31"
      },
      {
        "layer_type": "taxonomic_conv",
        "n_layers": 6,
        "kernel_size": 13,
        "stride": 2,
        "padding": 6,
        "_comment": "128x128x31 -> 64x64x(2^6-1) = 64x64x63"
      },
      {
        "layer_type": "taxonomic_conv",
        "n_layers": 7,
        "kernel_size": 13,
        "stride": 2,
        "padding": 6,
        "_comment": "64x64x63 -> 32x32x(2^7-1) = 32x32x127 (latent space)"
      }
    ],
    "decoder_layers": [
      {
        "layer_type": "taxonomic_deconv",
        "n_layers": 7,
        "kernel_size": 13,
        "stride": 2,
        "padding": 6,
        "output_padding": 1,
        "_comment": "32x32x255 -> 64x64x(2^7-1) = 64x64x127"
      },
      {
        "layer_type": "taxonomic_deconv",
        "n_layers": 6,
        "kernel_size": 13,
        "stride": 2,
        "padding": 6,
        "output_padding": 1,
        "_comment": "64x64x127 -> 128x128x(2^6-1) = 128x128x63"
      },
      {
        "layer_type": "taxonomic_deconv",
        "n_layers": 5,
        "kernel_size": 13,
        "stride": 2,
        "padding": 6,
        "output_padding": 1,
        "_comment": "128x128x63 -> 256x256x(2^5-1) = 256x256x31, then final conv to 256x256x3"
      }
    ]
  },
  "training": {
    "epochs": 20,
    "learning_rate": 0.0003
  },
  "analysis": {
  "checkpoint_path": "outputs/celebahq/training/celebahq_large_kernel/final_model.pt",
    "num_latent_batches": 50,
    "num_reconstruction_batches": 20,
    "num_multiple_recon_images": 8,
    "num_reconstructions_per_image": 8,
    "num_activation_images": 3
  },
  "output": {
    "training_save_dir": "outputs/celebahq/training/celebahq_large_kernel",
    "analysis_save_dir": "outputs/celebahq/analysis/celebahq_large_kernel"
  }
}